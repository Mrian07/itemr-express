        </activity>
<!--        <activity-->
<!--            android:name="com.fandy.aplikasibiodata.PengaturanActivity"-->
<!--            android:label="@string/title_activity_ubah" >-->
<!--        </activity>-->
        <activity
            android:name="com.fandy.aplikasibiodata.CetakActivity"
